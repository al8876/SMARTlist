<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Meta Information -->
    <title>compleet.io</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- External CSS -->
    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />

    <!-- Roboto Font -->
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed:300,300i,700,700i|Roboto:300,300i,700,700i" rel="stylesheet">

    <!-- Border Box CSS -->
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />

    <!--Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <!-- compleet Style Sheet CSS -->
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

    <!-- Create Format Package -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.20.1/moment.min.js"></script>
  </head>

  <body>

    <!-- Top nav bar (fixed) -->
    <nav id="nav-bar-ai">
      <span class="header-ai">compleet.ai</span>
    </nav>

    <!-- New List Item -->
    <section class="new-list">
      <form action="/" method="POST">
        <textarea name="text" placeholder="What do you want to do?"></textarea>
        <button type='submit'>
          ADD ITEM
        </button>
      </form>
    </section>

    <!-- Categories -->
    <section id='firstCategory' class='category watch'>
      <button id='toWatch' type="submit">TO WATCH</button>
      <section class='list-ai'>
        
        <div class='listItem'>
            <label class="check">
                <input type="checkbox" checked="checked">
                <span class="checkmark"></span>
              </label>
            <p>Shrek</p>
        </div>
       
        <div class='listItem'>
            <label class="check">
                <input type="checkbox" >
                <span class="checkmark"></span>
              </label>
            <p>Harry Potter</p>
        </div>

      </section>
    </section>


    <section class='category read'>
      <button id='toRead' type="submit">TO READ</button>
      <section class='list-ai'>
          <div class='listItem'>
              <label class="check">
                  <input type="checkbox" >
                  <span class="checkmark"></span>
                </label>
              <p>Good to Great</p>
          </div>
      </section>
    </section>

    <section class='category eat'>
      <button id='toEat' type="submit">TO EAT</button>
      <section class='list-ai'>
          <div class='listItem'>
              <label class="check">
                  <input type="checkbox" >
                  <span class="checkmark"></span>
                </label>
              <p>Cafe Medina</p>
          </div>
      </section>
    </section>


    <section class='category buy'>
      <button id='toBuy' type="submit">TO BUY</button>
      <section class='list-ai'>
      </section>
    </section>


    <section class='category other'>
      <button id='toOther' type="submit">OTHER</button>
      <section class='list-ai'>
      </section>
    </section>

    <section class='modal-ai'>

      <div class='modal-content'>
        <div class='modal-header'>
          <button class='modal-close'>X</button>
          <h2>Shrek</h2>
        </div>

        <div class='modal-category'>
          <p>Category:</p>
          <div class="dropdown">
            <button class="btn btn-secondary dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              Category
            </button>
            <div class="dropdown-menu">
              <a class="dropdown-item" href="#">Watch</a>
              <a class="dropdown-item" href="#">Read</a>
              <a class="dropdown-item" href="#">Eat</a>
              <a class="dropdown-item" href="#">Buy</a>
              <a class="dropdown-item" href="#">Other</a>
            </div>
          </div>
        </div>

        <div class='modal-complete'>
          <p>Done:</p>
          <div class="dropdown">
              <button class="btn btn-secondary dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                Category
              </button>
              <div class="dropdown-menu">
                <a class="dropdown-item" href="#">No</a>
                <a class="dropdown-item" href="#">Yes</a>
              </div>
            </div>
        </div>

        <div class='modal-body'>
          <h3>Movie Information:</h3>
        </div>
      </div>
    </section>   
    
    <!-- Bootstrap JS-->
    <script src="https://code.jquery.com/jquery-3.1.1.slim.min.js" integrity="sha384-A7FZj7v+d/sdmMqp/nOQwliLvUsJfDHW+k9Omg/a/EheAdgtzNs3hpfag6Ed950n" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js" integrity="sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn" crossorigin="anonymous"></script>
        
    <!-- JS APP -->
    <script type="text/javascript" src="/scripts/app.js"></script>
 
  </body>
</html>

