<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Meta Information -->
    <title>compleet.io</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- External CSS -->
    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />

    <!-- compleet Style Sheet CSS -->
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

     <!-- Font Awesome -->
     <script src="https://use.fontawesome.com/40bd759758.js"></script>

    <!-- Roboto Font -->
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed:300,300i,700,700i|Roboto:300,300i,700,700i" rel="stylesheet">

    <!-- Border Box CSS -->
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>

    <!-- JS APP -->
    <script type="text/javascript" src="/scripts/app.js"></script>

    <!-- Create Format Package -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.20.1/moment.min.js"></script>
  
  </head>

  <body>

    <!-- Top nav bar (fixed) -->
    <nav id="nav-bar">
      <span class="header">compleet.ai</span>
    </nav>

    <section class="new-list">
      <form action="/" method="POST">
        <textarea name="text" placeholder="What do you want to do?"></textarea>
        <button type='submit'>
          ADD ITEM
        </button>
      </form>
    </section>

    <section id='firstCategory' class='category watch'>
      <button id='toWatch' type="submit">TO WATCH</button>
      <section class='list'>
        
        <div>
            <label class="check">
                <input type="checkbox" checked="checked">
                <span class="checkmark"></span>
              </label>
            <p>Shrek</p>
        </div>
       
        <div>
            <label class="check">
                <input type="checkbox" >
                <span class="checkmark"></span>
              </label>
            <p>Harry Potter</p>
        </div>

      </section>
    </section>


    <section class='category read'>
      <button id='toRead' type="submit">TO READ</button>
      <section class='list'>
          <div>
              <label class="check">
                  <input type="checkbox" >
                  <span class="checkmark"></span>
                </label>
              <p>Good to Great</p>
          </div>
      </section>
    </section>


    <section class='category eat'>
      <button id='toEat' type="submit">TO EAT</button>
      <section class='list'>
      </section>
    </section>


    <section class='category buy'>
      <button id='toBuy' type="submit">TO BUY</button>
      <section class='list'>
      </section>
    </section>


    <section class='category other'>
      <button id='toOther' type="submit">OTHER</button>
      <section class='list'>
      </section>
    </section>

  </body>
</html>
